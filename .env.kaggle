# Kaggle/Cloud Deployment Configuration

# Mode
MODE=kaggle

# LLM Configuration (Free Gemini Flash via Vertex AI)
LLM_PROVIDER=gemini
LLM_MODEL=gemini-1.5-flash
LLM_TEMPERATURE=0.7
LLM_MAX_TOKENS=2048

# GCP Configuration
GCP_PROJECT_ID=${KAGGLE_GCP_PROJECT_ID}
GCP_REGION=us-central1

# Storage Configuration
STORAGE_TYPE=gcs
GCS_BUCKET_UPLOADS=${KAGGLE_BUCKET_UPLOADS}
GCS_BUCKET_OUTPUTS=${KAGGLE_BUCKET_OUTPUTS}

# Firestore
FIRESTORE_DATABASE=(default)

# Vector Database
VECTOR_DB_TYPE=vertex

# ADK Configuration
ADK_MODE=cloud

# Logging
LOG_LEVEL=INFO
